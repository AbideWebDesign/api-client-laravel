{
    "name": "civilservices/api",
    "homepage": "https://github.com/CivilServiceUSA/api-client-laravel",
    "description": "A Laravel 5 PHP Client for our Civil Services API Edit",
    "keywords": ["laravel", "api", "civil services"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Civil Services",
            "email": "hello@civil.services",
            "homepage":"https://civil.services"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "guzzlehttp/guzzle": "^6.2",
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7"
    },
    "suggest": {
        "phpunit/phpunit": "Allow to use PHPUnit for testing (~5.7)."
    },
    "autoload": {
        "psr-4": { "CivilServices\\Api\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "CivilServices\\Api\\Tests\\": "tests/" }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}