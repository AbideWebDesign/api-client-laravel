{
    "name": "civilservices/api-client-laravel",
    "homepage": "https://github.com/CivilServiceUSA/api-client-laravel",
    "description": "A Laravel 5 PHP Client for our Civil Services API Edit",
    "keywords": ["laravel", "api", "civil services"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Civil Services",
            "email": "hello@civil.services",
            "homepage":"https://civil.services"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "guzzlehttp/guzzle": "^6.2",
        "laravel/framework": "5.4.*",
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.1",
        "mockery/mockery": "^0.9.4",
        "satooshi/php-coveralls": "^1.0",
        "laravel/laravel": "5.4.*"
    },
    "autoload": {
        "psr-4": { "CivilServices\\Api\\": "src" }
    },
    "autoload-dev": {
        "psr-4": {
            "CivilServices\\Api\\Test\\": "tests"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "minimum-stability ": "dev",
    "prefer-stable": true
}